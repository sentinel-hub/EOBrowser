  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "prettier": "prettier --write \"src/**/*.js\"",
    "start": "react-scripts start",
    "build": "node -r dotenv/config ./node_modules/.bin/react-scripts build dotenv_config_path=config/${BUILD_ENV}.env  ",
    "build:testing": "BUILD_ENV=testing npm run build",
    "build:staging": "BUILD_ENV=staging npm run build",
    "build:production": "BUILD_ENV=production npm run build",
    "lint": "eslint --max-warnings 0 --ext .js src/",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "see": "node -e 'console.log(require(\"process\").env)'",
    "translate": "yarn translate-en",
    "translate-en": "npx ttag update --extractLocation=file i18n/en.po src/ && npx ttag po2json i18n/en.po > i18n/en.po.json"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
